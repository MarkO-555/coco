10 'PROGRAM TO FIND START, END20 'AND TRANSFER ADDRESSES OF30 'A DISK MACHINE LANGUAGE40 'FILE70 CLEAR300:DIM A(5):CLS:S=1:TB=0:FIRST=1110 INPUT"<ENTER> FILE NAME";F$:IF F$=""THENSTOP120 INPUT"OUTPUT TO PRINTER (Y/N)";R$130 IF LEFT$(R$,1)="Y" THENUN=-2 ELSEUN=0140 PRINT#UN,"FILE NAME ";F$150 PRINT:PRINT#UN,"( ALL ADDRESSES IN HEX )"160 OPEN"D",#1,F$170 BP=1:GOSUB330180 IF A(1)<>0 THEN440190 BC=A(2)*256+A(3):SA=A(4)*256+A(5)200 IF FIRST THENFA=SA:FIRST=0:GOTO220210 IF LA+1<>SA THENGOSUB420:FA=SA220 LA=SA+BC-1230 TB=TB+BC+5240 SC=FIX((TB)/256):BP=(TB)-(SC*256)+1250 S=SC+1260 GOSUB330270 IF A(1)=0 THEN190280 IF A(1)<>&HFF OR A(2)<>0 OR A(3)<>0 THEN440290 TA=A(4)*256+A(5)300 GOSUB420310 PRINT#UN,"TRANSFER = ";HEX$(TA)320 CLOSE:END330 GOSUB400340 FORI=1 TO 5350 IF BP>128 THENBB=BP-128:D1$=D2$ ELSEBB=BP360 A(I)=ASC(MID$(D1$,BB,1)):BP=BP+1370 IF BP>256 THENBP=1:S=S+1:GOSUB400380 NEXTI390 RETURN400 FIELD#1,128 AS D1$, 128 AS D2$:GET#1,S410 RETURN420 PRINT#UN,"START = ",HEX$(FA);"END = ";HEX$(LA)430 RETURN440 PRINT"error in file-type"450 STOP